<!-- _ResultadosBusquedaPartial.cshtml -->
@model List<AgroservicioCuxil.Models.Producto>

<h2>Resultados de Búsqueda</h2>

<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Accion</th>
            <!-- Otras columnas que desees mostrar -->
        </tr>
    </thead>
    <tbody>
        @foreach (var producto in Model)
        {
            <tr>
                <td>@producto.Nombre</td>
                <td>@producto.IdMarca</td>
                <td>
                    <button class="btn btn-primary">
                        Seleccionar
                    </button>
                </td>
            </tr>
        }
    </tbody>
</table>
