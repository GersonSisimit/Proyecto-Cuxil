@{
    ViewData["Title"] = "Definir Detalles";


}


<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            @*            <button id="myButton" class="btn btn-primary">Mostrar SweetAlert</button>
            *@     Gestion De Detalles De Productos
        </h1>
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-md-4">
                <div class="box box-info animated slideInRight">
                    <div class="box-header with-border">
                        <h3 class="box-title"><i class="fa fa-cubes"></i> Productos</h3>
                    </div>
                    <div class="box-body">
                        <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu">
                            <li class="nav-item">
                                <a href="#" class="nav-link active" data-toggle="collapse">
                                    <i class="fa fa-shopping-basket" aria-hidden="true"></i>
                                    <p>
                                        Productos
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview collapse">
                                    <li class="nav-item">
                                        <a href="@Url.Action("MarcaProductos", "Productos")" class="nav-link">
                                            <i class="fa fa-shopping-bag" aria-hidden="true"></i>
                                            <p>Marca de productos</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="@Url.Action("TipoProducto", "Productos")" class="nav-link">
                                            <i class="fa fa-plus" aria-hidden="true"></i>
                                            <p>Tipos de producto</p>
                                        </a>
                                    </li> 
                                    <li class="nav-item">
                                        <a href="@Url.Action("SubtipoProducto", "Productos")" class="nav-link">
                                            <i class="fa fa-sitemap" aria-hidden="true"></i>
                                            <p>Subtipos de producto</p>
                                        </a>
                                    </li> 
                                    <li class="nav-item">
                                        <a href="@Url.Action("PresentacionesProducto", "Productos")" class="nav-link">
                                            <i class="fa fa-sliders" aria-hidden="true"></i>
                                            <p>PresentacionesProducto</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>

                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
@section Scripts {
    <script src="~/plugins/sweetalert2/sweetalert2.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#myButton').click(function () {
                Swal.fire({
                    title: 'SweetAlert!',
                    text: '¡Este es un mensaje de SweetAlert!',
                    icon: 'success',
                    confirmButtonText: 'Aceptar'
                });
            });
        });
    </script>
}
    @section Styles {
    <link href="~/plugins/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
}
